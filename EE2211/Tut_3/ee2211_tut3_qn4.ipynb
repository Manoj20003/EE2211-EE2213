    "from collections import defaultdict\n",
    "\n",
    "\n",
    "def dice():\n",
    "    d = defaultdict(int)\n",
    "    sums = [] \n",
    "    outcomes = range(1, 7)\n",
    "\n",
    "    for i in outcomes:\n",
    "        for j in outcomes:\n",
    "            sums.append([i + j, [i, j]])\n",
    "\n",
    "    for x in sums:\n",
    "        d[x[0]] += x[1]\n",
    "\n",
    "    return sums\n",
    "\n",
    "\n",
    "dice()"
